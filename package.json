{
  "name": "argon-design-system-react",
  "version": "1.1.0",
  "description": "React version of Argon Design System by Creative Tim",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-design-system-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "design",
    "design-react",
    "argon-design",
    "argon-design-react",
    "kit",
    "react-kit",
    "argon-design-system",
    "argon-design-system-react",
    "design-system-react"
  ],
  "author": "Vincent Lucero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-design-system-react/issues"
  },
  "scripts": {
    "start": "run-p --race dev:firebase dev:react",
    "dev:firebase": "firebase serve -p 4000",
    "dev:react": "NODE_PATH=./src react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@reach/router": "^1.3.4",
    "@stripe/stripe-js": "^1.8.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "cors": "^2.8.5",
    "envvar": "^2.0.0",
    "firebase": "^7.17.2",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.9.0",
    "headroom.js": "0.11.0",
    "moment": "^2.24.0",
    "node-sass": "7.0.0",
    "nouislider": "14.1.1",
    "npm-install-all": "^1.1.21",
    "npm-run-all": "^4.1.5",
    "plaid": "^7.0.0",
    "react": "16.12.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.12.0",
    "react-firebase-hooks": "^2.2.0",
    "react-firebaseui": "^4.1.0",
    "react-json-to-html": "0.0.3",
    "react-load-script": "0.0.6",
    "react-plaid-link": "^3.0.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.0",
    "react-spinners": "^0.9.0",
    "reactstrap": "8.4.1",
    "stripe": "^8.81.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.19",
    "eslint-plugin-flowtype": "3.13.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "jquery": "3.4.1",
    "typescript": "3.7.5"
  },
  "proxy": "http://localhost:4000"
}
